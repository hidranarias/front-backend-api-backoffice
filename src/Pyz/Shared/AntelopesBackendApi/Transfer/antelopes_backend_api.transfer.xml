<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="AntelopesBackendApiAttributes" strict="true">
        <property name="idAntelope" type="int"/>
        <property name="name" type="string"/>
        <property name="typeId" type="int"/>
        <property name="locationId" type="int"/>
        <property name="age" type="int"/>
        <property name="weight" type="float"/>
        <property name="gender" type="string"/>
    </transfer>

    <transfer name="AntelopeTypesBackendApiAttributes" strict="true">
        <property name="id" type="int"/>
        <property name="typeName" type="string"/>
        <property name="description" type="string"/>
    </transfer>

    <transfer name="AntelopeLocationsBackendApiAttributes" strict="true">
        <property name="id" type="int"/>
        <property name="locationName" type="string"/>
        <property name="latitude" type="float"/>
        <property name="longitude" type="float"/>
        <property name="description" type="string"/>
    </transfer>

    <transfer name="RestAntelopesAttributes" strict="true"/>
</transfers>
